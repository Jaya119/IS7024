@page
@model TravelAdvisor.Pages.DataIntegrationModel
@{
    ViewData["Title"] = "Data Integration";
    var states = (QuickType.State[])ViewData["State"];

}

<table border="1" width="90%">
    <tr><th>Name</th><th>Abbreviation</th></tr>
    @{
        foreach (QuickType.State state in states)
        {
            <tr>
                <td>@state.Name</td>
                <td>@state.Abbreviation</td>

            </tr>
        }
    }


</table>
